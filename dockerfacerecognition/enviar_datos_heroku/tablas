usuarios
create table if not exists web_usuarios (cedula integer, nombre varchar(150), contrato_id varchar(150))


interacciones 
'create table if not exists web_interacciones (nombre varchar(150), fecha date, hora time without time zone, razon varchar(150), contrato varchar(150), cedula_id integer)'


fotos
create table if not exists web_fotos (foto varchar(150), cedula_id integer)


horariospermitidos
'create table if not exists web_horariospermitidos (entrada time without time zone, salida time without time zone, cedula_id integer, dia_id varchar(180))'

